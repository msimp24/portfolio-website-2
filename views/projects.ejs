<div class="page-hero">
  <div class="container">
    <div class="header">
      <h1>My Projects</h1>
      <p>A showcase of projects that I've built</p>
    </div>
  </div>
</div>

<div class="container">
  <div class="row">
    <div class="col">
      <div class="projects-grid">
        <% projects.forEach((project) =>{%>

        <a class="project-link" href="/projects/<%-project.slug%>">
          <div class="project-card">
            <img src="<%-project.img%>" alt="" />
            <div>
              <h3><%-project.title %></h3>
              <p><%-project.desc %></p>
              <div class="tech-wrapper">
                <% project.tech.forEach((tech) =>{%>

                <span class="tech-pill"><%-tech %> </span>

                <% }) %>
              </div>
            </div>
          </div>
        </a>
        <% })%>
      </div>
    </div>
  </div>
</div>
